# App description
## App instanciation
o:MachineBrowser_42
### appUri <http://eatld.et.tu-dresden.de/ADL#MachineBrowser_42>

a adl:App;

## App information
adl:appName "Machine Browser";
rdfs:label "MachineBrowser";
adl:appVersionString 1.0;
adl:appVersion 345;
adl:textualDescription 
  """
  This app enables the user to access exemplary structural 
  configuration data and live data via the data harmonization middleware. 
  It provides access to the machineӳ data and sensors via the technical hierarchy of the machine.
  """;

## SPARQL Template
adl:hasSparqlTemplate [
  a adl:SparqlTemplate;
  adl:queryString 
  """
  PREFIX objectOntology: %%%objectOntology%%%
  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
  
  SELECT ?data FROM %%%endpointURI%%%
      WHERE {
          ?machine a objectOntology:%%%machineURI%%%;
              objectOntology:data ?data.
      }
  """;
  adl:queryKey "templateA";
  adl:hasPlaceholder [
    a adl:TemplatePlaceholder;
	rdfs:label "machineURI";
	adl:isDeliveredBy _:IOparameter1
	]
];

## Abilities
adl:providesAbility [
  a adl:Ability;
  adl:textualDescription "Read machine state and parameters";
  adl:consistsOfAction [
	a adl:ReadAction;
	a adl:SingleTargetAction;
	adl:actsOnTarget [a adl:MachineState];
	adl:actsOnTarget [a adl:MachineParameter]
  ];
  adl:needsComponent [a adl:NetworkConnection]
];
adl:providesAbility [
  a adl:Ability;
  adl:textualDescription "View machine state and parameters";
  adl:consistsOfAction [
	a adl:ViewAction;
	a adl:SingleTargetAction;
	adl:actsOnTarget [a adl:MachineState];
	adl:actsOnTarget [a adl:MachineParameter]
  ];
  adl:needsComponent [a adl:NetworkConnection]
];
adl:providesAbility [
  a adl:Ability;
  adl:textualDescription "Browse machine state and parameters";
  adl:consistsOfAction [
	a adl:BrowseAction;
	a adl:SingleTargetAction;
	adl:actsOnTarget [a adl:MachineState];
	adl:actsOnTarget [a adl:MachineParameter]
  ];
  adl:needsComponent [a adl:NetworkConnection]
];

